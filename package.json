  "main": "server/dist/index.js",
  "engines": {
    "node": "12.19.0"
  },
  "scripts": {
    "start:production": "cd server/ && node dist/index.js",
    "start:dev": "concurrently --kill-others \"cd client/ && npm start\" \"cd server && npm run dev\"",
    "front:install:build": "cd client/ && npm i && npm run build",
